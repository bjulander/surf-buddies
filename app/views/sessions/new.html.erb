<h1> Log In</h1>

<%= render(partial: "application/errors", locals: {errors: @errors})%>

<%= form_tag login_path do %>

      <%= label_tag(:username, "Username:") %>
      <%= text_field_tag(:username) %>

      <%= label_tag(:password, "Password:") %>
      <%= password_field_tag(:password) %>

      <%= submit_tag("Log In") %>
  <% end %> 

  <p><div class="fb-login-button" data-width="" data-size="medium" data-button-type="login_with" data-layout="rounded" data-auto-logout-link="false" data-use-continue-as="true"></div></p>
  <p>Log In <%= link_to ' with Google', '/auth/google_oauth2'%>.</p>